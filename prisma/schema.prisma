// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @unique
  hashPsw   String
  hashToken String?
  version   Int
  createdAt Int
  updatedAt Int

  @@map("users")
}
